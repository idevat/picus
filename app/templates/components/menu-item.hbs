{{#if isCollapsable}}
  <span class='menu-item-collapse' {{action (toggle 'isCollapsed' this)}}>
    {{#if isCollapsed}}+{{else}}-{{/if}}
  </span>
{{/if}}

{{#if onCheckAction}}
  {{one-way-checkbox isChecked update=(action toggleCheck)}}
{{/if}}
{{#link-to targetRoute componentId}}{{title}}
  {{#if status}}
    [{{status}}]
  {{/if}}
{{/link-to}}

{{#unless isCollapsed}}
  {{yield}}
{{/unless}}
