{{#each levels as |level|}}
  {{#bs-accordion selected="1" as |acc|}}
    {{#acc.item value="1" title=level}}
      {{agent-properties-sections parameters=metadata.parameters filter=filter level=level form=form}}
    {{/acc.item}}
  {{/bs-accordion}}
{{/each}}


{{#bs-accordion selected="1" as |acc|}}
  {{#acc.item value="1" title="undefined level"}}
    {{agent-properties-sections parameters=metadata.parameters filter=filter form=form}}
  {{/acc.item}}
{{/bs-accordion}}
