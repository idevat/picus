{{#ember-wormhole to="breadcrumb"}}
  <!-- demo of breadcrumb; @todo: create a real component if used -->
  <ol class="breadcrumb">
    <li class="breadcrumb-item">{{#link-to "cluster.properties"}}Home{{/link-to}}</li>
    <li class="breadcrumb-item active">Properties</li>
  </ol>
{{/ember-wormhole}}

{{properties-form
  data=model.staticCluster.properties

  onSubmitAction=(action 'submitProperties')
  onSearchRefresh=(action 'onSearch')
  onAdvancedAction=(action 'onAdvanced')
  onInternalAction=(action 'onInternal')

  presetFilter=model.filterProperties
  showAdvanced=model.showAdvanced
}}
